{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.local",
    "serve": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,less,postcss,css,scss}\"",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged",
    "lint:pretty": "pretty-quick --staged",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "cz": "git-cz",
    "prepare": "husky install",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf yarn.lock && rimraf package-lock.json && rimraf node_modules && pnpm bootstrap"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ls-lint/ls-lint": "^1.11.2",
    "@nuxtjs/tailwindcss": "^6.6.7",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "9.4.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.4.3",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.5.3",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0"
  },
  "dependencies": {
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.10",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "ant-design-vue": "^3.2.20",
    "pinia": "^2.0.36",
    "unplugin-vue-components": "^0.24.1"
  }
